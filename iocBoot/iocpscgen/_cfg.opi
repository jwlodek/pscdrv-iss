            <scriptName>Check dir size</scriptName>
            <scriptText><![CDATA[import os

def get_size(start_path):
    total_size = 0
    for dirpath, dirnames, filenames in os.walk(start_path):
        for f in filenames:
            fp = os.path.join(dirpath, f)
            total_size += os.path.getsize(fp)
    return total_size

home = os.getenv("HOME")
path = home+'/EncoderData'

try:
	statinfo = get_size(path)
	if statinfo > 0:
		widget.setPropertyValue("enabled", 'true')
except:
	widget.setPropertyValue("enabled", 'false')]]></scriptText>
