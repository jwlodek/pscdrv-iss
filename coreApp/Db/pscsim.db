# offset 12 is a bit register.

record(bo, "$(P)reg3:0-Sel") {
  field(DTYP, "PSC Reg")
  field(OUT , "@test 1 12")
  field(MASK, "0x40000") # bit 18
  field(ZNAM, "False")
  field(ONAM, "True")
}

record(mbbo, "$(P)reg3:1-Sel") {
  field(DTYP, "PSC Reg")
  field(OUT , "@test 1 12")
  field(NOBT, "2")
  field(SHFT, "16") # select bits 16 and 17
  field(ZRST, "Zero")
  field(ONST, "One")
  field(TWST, "Two")
  field(THST, "Three")
  field(ZRVL, "0")
  field(ONVL, "1")
  field(TWVL, "2")
  field(THVL, "3")
}

