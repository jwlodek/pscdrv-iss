# ============= ADC10k:0 - 16 ============= #
record(waveform, "$(PSy){$(PS)}ADC10k:0_Bin_") {
	field(DTYP, "PSC Block I16 In")
	field(INP , "@Rx$(PS) 16")
	field(SCAN, "I/O Intr")
	field(FTVL, "DOUBLE")
	field(NELM, "$(ADC10K_POINTS)")
	field(FLNK, "$(PSy){$(PS)}ADC10k:0_asub_")
}

record(aSub, "$(PSy){$(PS)}ADC10k:0_asub_")
{
	field(SNAM, "subADCWfProc")
	field(INPA, "$(PSy){$(PS)}ADC10k:0_Bin_.VAL")
	field(FTA,  "DOUBLE")
	field(NOA,  "$(ADC10K_POINTS)")
	field(INPB, "$(PSy){$(PS)}ADC10k:0_Bin_.NORD")
	field(FTB,  "LONG")
	field(NOB,  "1")
	field(INPC, "$(PSy){$(PS)}ADC10k:0_Bin_.NELM")
	field(FTC,  "LONG")
	field(NOC,  "1")
	field(FTVA, "FLOAT")
	field(NOVA, "$(ADC10K_POINTS)")
	field(OUTA, "$(PSy){$(PS)}ADC10k:0_Raw-I PP")
	field(FLNK, "")
}

record(waveform, "$(PSy){$(PS)}ADC10k:0_Raw-I")
{
	field(NELM, "$(ADC10K_POINTS)")
	field(FTVL, "FLOAT")
	
	# sync timestamp with the dedicated record whose timestamp correspond to
	# a previos cycle start (BT0)
	field(TSEL, "$(TIMESTAMP).TIME")
}

