    field(INPB, "$(P)Aie$(ID):S2-SP.VAL")
	field(INPC, "$(P)Aie$(ID):S3-SP.VAL")
	field(INPD, "$(P)Aie$(ID):Pos-x1")
	field(INPE, "$(P)Aie$(ID):Pos-x2")
#|(x2-x1)/(s2-s1)|
    field(CALC, "D+(E-D)*(C-A)/(B-A)")
    field(EGU,  "mm")
    field(HOPR, "10")
    field(LOPR, "-10")
    field(PREC, "3")
