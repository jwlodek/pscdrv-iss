record(calc, "$(P)Aie$(ID):Pos-x1") {
	field(SCAN, ".2 second")
	field(CALC, "A*B-C")
	field(INPA, "$(P)Aie$(ID):Pos-Arry-x1.VAL PP")	
	field(INPB, "0.000001")
	field(INPC, "$(P)AI$(ID):BBA_Offset_X1-SP.VAL PP")
	field(PREC, "3")
	field(EGU,  "mm")
}
record(ai, "$(P)Aie$(ID):Pos-x1_") {
    field(INP,"$(P)Aie$(ID):Pos-x1")
}
record(calc, "$(P)Aie$(ID):x1-Sts-I") {
	field(SCAN, "1 second")
	field(CALC, "(B-A)*(B-A)")
	field(INPA, "$(P)Aie$(ID):Pos-x1_ NPP")
	field(INPB, "$(P)Aie$(ID):Pos-x1_ PP")
	field(LOLO , "0.0")
	field(LOW , "0.0")
	field(HIGH, "1.0")
	field(PREC, "3")
	field(HSV , "INVALID")
	field(LSV , "INVALID")
	field(LLSV, "INVALID")
}
