### Post Morterm
### 480 x 4 = 1920  OFFSET
### 480 x 8 = 3840
### 480 x 16 = 7680
record(waveform, "$(P,undefined):PM-BPM$(CNO,undefined)-$(N,undefined)X-I") {
        field(DESC, "$(P,undefined){CC}-$(N,undefined) X position")
        field(DTYP, "PSC Block I32 In")
        field(INP , "@aie_wfm_rx_1 76 $(OFFSET_X,undefined) 3840")
        field(SCAN, "I/O Intr")
        field(FTVL, "DOUBLE")
        field(NELM, "30000")
}

record(waveform, "$(P,undefined):PM-BPM$(CNO,undefined)-$(N,undefined)Y-I") {
        field(DESC, "$(P,undefined)-$(CNO,undefined)-$(N,undefined) Y position")
        field(DTYP, "PSC Block I32 In")
        field(INP , "@aie_wfm_rx_1 76 $(OFFSET_Y,undefined) 3840")
        field(SCAN, "I/O Intr")
        field(FTVL, "DOUBLE")
        field(NELM, "30000")
}
