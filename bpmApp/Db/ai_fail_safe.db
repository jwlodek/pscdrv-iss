

########################
##  0 : SDI, 1: Sim
record(calcout,"SR:C31-{AI}Sim:Enable-Cal") {
        field(INPA, "SR:C31-{AI}Sim:Enable-SP.VAL CP")
        field(INPB, "SR:C03-BI{DCCT:1}I:Total-I CP")
        field(CALC,"B>0.2 && A=1? 0:1")
        field(OOPT,"When Zero")
        field(OUT, "SR:C31-{AI}Sim:Enable-SP.VAL CA")
        field(FLNK, "Sim:Enable-Cal2")
}

record(calcout,"SR:C31-{AI}Sim:Enable-Cal2") {
        field(INPB, "SR:C03-BI{DCCT:1}I:Total-I CP")
        field(CALC,"B>0.2?1:0")
        field(OUT, "SR:C31-{AI}Sim:Enable-SP.DISP CA")
}

